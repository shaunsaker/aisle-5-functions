{
  "name": "aisle-5-functions",
  "version": "1.0.0",
  "scripts": {
    "env:dev": "cp ./config/development/* ./src/firebase && firebase use development",
    "env:prod": "cp ./config/production/* ./src/firebase && firebase use production",
    "predev": "yarn env:dev",
    "dev": "ts-node-transpile-only",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "firebase serve --only functions",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --ext [.ts,.tsx] .",
    "lint:prettier": "prettier -c \"**/*.{ts,tsx}\"",
    "lint:tsc": "tsc",
    "test": "jest",
    "predeploy": "yarn lint && yarn test && rimraf ./dist && yarn run build",
    "deploy": "firebase deploy --only functions"
  },
  "main": "dist/index.js",
  "dependencies": {
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.15.5",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/jest": "^27.0.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "firebase-tools": "^9.18.0",
    "jest": "^27.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  },
  "resolutions": {
    "@types/serve-static": "1.13.9"
  },
  "private": true
}
